{
  "name": "mozartk/process-checker",
  "description": "Get list of running processes by process name, supports Windows and unix, macOS",
  "keywords": [
    "check",
    "monitor",
    "process"
  ],
  "homepage": "https://github.com/mozartk",
  "license": "MIT",
  "authors": [
    {
      "name": ":mozartk",
      "email": "mozartk30@gmail.com",
      "homepage": "https://github.com/mozartk"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mozartk/process-handler",
      "no-api": true
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "monolog/monolog": "~1.13",
    "craftpip/process-handler": "dev-master",
    "hassankhan/config" : "~0.11",
    "symfony/yaml" : "~2",
    "matomo/ini" : "~2"
  },
  "require-dev": {
    "phpunit/phpunit": "^3.7|~4|~5|~6"
  },
  "autoload": {
    "psr-4": {
      "mozartk\\processCheck\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mozartk\\processCheck\\Test\\": "tests"
    }
  }
}
